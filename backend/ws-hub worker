      await db.update(aiUsageLogs).set({
        requestCount: sql`${aiUsageLogs.requestCount} + 1`,
        lastRequestAt: (/* @__PURE__ */ new Date()).toISOString()
      }).where(eq(aiUsageLogs.id, usageResult[0].id));
