report.get("/", jwt({ alg: "HS256" }), zValidator("query", external_exports.object({
  page: external_exports.coerce.number().int().min(1, "\u9875\u7801\u5FC5\u987B\u5927\u4E8E0").default(1),
  pageSize: external_exports.coerce.number().int().min(1, "\u6BCF\u9875\u6570\u91CF\u5FC5\u987B\u5927\u4E8E0").max(100, "\u6BCF\u9875\u6570\u91CF\u4E0D\u80FD\u8D85\u8FC7100").default(10),
  faultCategory: external_exports.string().optional(),
  faultTagIds: external_exports.string().transform((val) => {
    if (!val)
      return [];
    try {
      return val.split(",").map((id) => parseInt(id.trim())).filter((id) => !isNaN(id));
    } catch {
      return [];
    }
  }).optional(),
  startDate: external_exports.string().optional(),
  endDate: external_exports.string().optional(),
  reporter: external_exports.string().optional(),
  sortBy: external_exports.enum(["id", "deviceName", "faultCategory", "createdAt", "durationMinutes"]).default("createdAt"),
  sortOrder: external_exports.enum(["asc", "desc"]).default("desc")
})), async (c) => {
  const { page, pageSize, faultCategory, faultTagIds, startDate, endDate, reporter, sortBy, sortOrder } = c.req.valid("query");
  const db = getDB(c);
  try {
    const conditions = [eq(reports.isDeleted, 0)];
    if (faultCategory) {
      conditions.push(eq(reports.faultCategory, faultCategory));
    }
    if (startDate) {
      conditions.push(gte(reports.createdAt, startDate));
    }
    if (endDate) {
      conditions.push(lte(reports.createdAt, endDate));
    }
    if (reporter) {
      conditions.push(eq(reports.reporter, reporter));
    }
    const whereCondition = and2(...conditions);
    let tagFilterCondition = void 0;
    if (faultTagIds && faultTagIds.length > 0) {
      tagFilterCondition = sql`EXISTS (
        SELECT 1 FROM json_each(${reports.faultTags}) AS tag 
        WHERE tag.value IN (${faultTagIds.join(",")})
      )`;
    }
    let countQuery = db.select({ count: sql`count(*)` }).from(reports).where(whereCondition);
    if (tagFilterCondition) {
      countQuery = countQuery.where(tagFilterCondition);
    }
    const countResult = await countQuery;
    const total = countResult[0].count;
    const offset = (page - 1) * pageSize;
    const orderBy = sortOrder === "desc" ? desc(reports[sortBy]) : reports[sortBy];
    let reportsQuery = db.select({
      id: reports.id,
      deviceName: reports.deviceName,
      faultCategory: reports.faultCategory,
      faultTags: reports.faultTags,
      durationMinutes: reports.durationMinutes,
      reporter: reports.reporter,
      createdAt: reports.createdAt,
      authorId: reports.authorId
    }).from(reports).where(whereCondition);
    if (tagFilterCondition) {
      reportsQuery = reportsQuery.where(tagFilterCondition);
    }
    const reportsResult = await reportsQuery.orderBy(orderBy).limit(pageSize).offset(offset);
    const allTagIds = /* @__PURE__ */ new Set();
    reportsResult.forEach((report2) => {
      if (report2.faultTags) {
        try {
          const tags = JSON.parse(report2.faultTags);
          tags.forEach((id) => allTagIds.add(id));
        } catch (e) {
          console.error("\u89E3\u6790\u6807\u7B7EID\u5931\u8D25:", e);
        }
      }
    });
    let tagDetails = [];
    if (allTagIds.size > 0) {
      tagDetails = await db.select({
        id: faultTags.id,
        name: faultTags.name,
        color: faultTags.color
      }).from(faultTags).where(inArray(faultTags.id, Array.from(allTagIds)));
    }
    const tagMap = /* @__PURE__ */ new Map();
    tagDetails.forEach((tag2) => {
      tagMap.set(tag2.id, tag2);
    });
    const list = reportsResult.map((report2) => {
      let tags = [];
      if (report2.faultTags) {
        try {
          const tagIds = JSON.parse(report2.faultTags);
          tags = tagIds.map((id) => tagMap.get(id)).filter(Boolean);
        } catch (e) {
          console.error("\u89E3\u6790\u6807\u7B7E\u5931\u8D25:", e);
        }
      }
      return {
        ...report2,
        tags
      };
    });
    return c.json({
      success: true,
      message: "\u83B7\u53D6\u62A5\u544A\u5217\u8868\u6210\u529F",
      data: {
        list,
        pagination: {
          page,
          pageSize,
          total,
          totalPages: Math.ceil(total / pageSize)
        }
      }
    });
  } catch (error) {
    console.error("\u83B7\u53D6\u62A5\u544A\u5217\u8868\u5931\u8D25:", error);
    return c.json({
      success: false,
      message: "\u83B7\u53D6\u62A5\u544A\u5217\u8868\u5931\u8D25",
      data: null
    }, 500);
  }
});
report.get("/:id", jwt({ alg: "HS256" }), zValidator("param", external_exports.object({
  id: external_exports.coerce.number().int().positive("ID\u5FC5\u987B\u4E3A\u6B63\u6574\u6570")
})), async (c) => {
  const { id } = c.req.valid("param");
  const db = getDB(c);
  try {
    const reportResult = await db.select().from(reports).where(
      and2(
        eq(reports.id, id),
        eq(reports.isDeleted, 0)
      )
    ).limit(1);
    if (reportResult.length === 0) {
      return c.json({
        success: false,
        message: "\u62A5\u544A\u4E0D\u5B58\u5728",
        data: null
      }, 404);
    }
    const report2 = reportResult[0];
    let tags = [];
    if (report2.faultTags) {
      try {
        const tagIds = JSON.parse(report2.faultTags);
        if (tagIds.length > 0) {
          const tagDetails = await db.select({
            id: faultTags.id,
            name: faultTags.name,
            color: faultTags.color
          }).from(faultTags).where(inArray(faultTags.id, tagIds));
          tags = tagDetails;
        }
      } catch (e) {
        console.error("\u89E3\u6790\u6807\u7B7E\u5931\u8D25:", e);
      }
    }
    const aiAnalyses2 = await db.select({
      id: sql`ai_analyses.id`,
      analysisType: sql`ai_analyses.analysis_type`,
      createdAt: sql`ai_analyses.created_at`,
      tokensUsed: sql`ai_analyses.tokens_used`
    }).from(sql`ai_analyses`).where(eq(sql`ai_analyses.report_id`, id)).orderBy(desc(sql`ai_analyses.created_at`));
    return c.json({
      success: true,
      message: "\u83B7\u53D6\u62A5\u544A\u8BE6\u60C5\u6210\u529F",
      data: {
        ...report2,
        tags,
        aiAnalyses: aiAnalyses2
      }
    });
  } catch (error) {
    console.error("\u83B7\u53D6\u62A5\u544A\u8BE6\u60C5\u5931\u8D25:", error);
    return c.json({
      success: false,
      message: "\u83B7\u53D6\u62A5\u544A\u8BE6\u60C5\u5931\u8D25",
      data: null
    }, 500);
  }
});
report.post("/", jwt({ alg: "HS256" }), zValidator("json", external_exports.object({
  deviceName: external_exports.string().min(1, "\u8BBE\u5907\u540D\u79F0\u4E0D\u80FD\u4E3A\u7A7A"),
  faultCategory: external_exports.string().min(1, "\u6545\u969C\u7C7B\u522B\u4E0D\u80FD\u4E3A\u7A7A"),
  faultTagIds: external_exports.array(external_exports.number().int().positive("\u6807\u7B7EID\u5FC5\u987B\u4E3A\u6B63\u6574\u6570")).optional(),
  durationMinutes: external_exports.number().int().min(0, "\u7EF4\u4FEE\u65F6\u957F\u4E0D\u80FD\u4E3A\u8D1F\u6570").optional(),
  description: external_exports.string().min(1, "\u7EF4\u4FEE\u63CF\u8FF0\u4E0D\u80FD\u4E3A\u7A7A"),
  attachments: external_exports.array(external_exports.string()).optional()
})), async (c) => {
  const payload = c.get("jwtPayload");
  const { deviceName, faultCategory, faultTagIds, durationMinutes, description, attachments } = c.req.valid("json");
  const db = getDB(c);
  try {
    if (payload.role === "viewer") {
      return c.json({
        success: false,
        message: "\u6743\u9650\u4E0D\u8DB3",
        data: null
      }, 403);
    }
    if (faultTagIds && faultTagIds.length > 0) {
      const tagCountResult = await db.select({ count: sql`count(*)` }).from(faultTags).where(inArray(faultTags.id, faultTagIds));
      if (tagCountResult[0].count !== faultTagIds.length) {
        return c.json({
          success: false,
          message: "\u5305\u542B\u4E0D\u5B58\u5728\u7684\u6807\u7B7E",
          data: null
        }, 400);
      }
    }
    const result = await db.insert(reports).values({
      deviceName,
      faultCategory,
      faultTags: faultTagIds ? JSON.stringify(faultTagIds) : null,
      durationMinutes,
      description,
      attachments: attachments ? JSON.stringify(attachments) : null,
      reporter: payload.username,
      authorId: payload.userId
    }).returning({
      id: reports.id,
      deviceName: reports.deviceName,
      faultCategory: reports.faultCategory,
      faultTags: reports.faultTags,
      durationMinutes: reports.durationMinutes,
      description: reports.description,
      attachments: reports.attachments,
      reporter: reports.reporter,
      createdAt: reports.createdAt
    });
    const newReport = result[0];
    if (faultTagIds && faultTagIds.length > 0) {
      await db.update(faultTags).set({
        usageCount: sql`${faultTags.usageCount} + 1`
      }).where(inArray(faultTags.id, faultTagIds));
    }
    return c.json({
      success: true,
      message: "\u521B\u5EFA\u62A5\u544A\u6210\u529F",
      data: newReport
    });
  } catch (error) {
    console.error("\u521B\u5EFA\u62A5\u544A\u5931\u8D25:", error);
    return c.json({
      success: false,
      message: "\u521B\u5EFA\u62A5\u544A\u5931\u8D25",
      data: null
    }, 500);
  }
});
