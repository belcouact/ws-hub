var hashPassword = /* @__PURE__ */ __name(async (password) => {
  const saltRounds = 10;
  return import_bcryptjs.default.hash(password, saltRounds);
}, "hashPassword");
var verifyPassword = /* @__PURE__ */ __name(async (password, hashedPassword) => {
  return import_bcryptjs.default.compare(password, hashedPassword);
}, "verifyPassword");

