// src/db/schema.ts
var schema_exports = {};
__export(schema_exports, {
  aiAnalyses: () => aiAnalyses,
  aiUsageLogs: () => aiUsageLogs,
  faultTags: () => faultTags,
  reports: () => reports,
  users: () => users
});
var users = sqliteTable("users", {
  id: integer("id").primaryKey({ autoIncrement: true }),
  username: text("username").notNull().unique(),
  email: text("email").unique(),
  password: text("password").notNull(),
  role: text("role", { enum: ["viewer", "editor", "admin"] }).default("viewer").notNull(),
  createdAt: text("created_at").notNull().default(sql`CURRENT_TIMESTAMP`),
  updatedAt: text("updated_at").notNull().default(sql`CURRENT_TIMESTAMP`)
});
var faultTags = sqliteTable("fault_tags", {
  id: integer("id").primaryKey({ autoIncrement: true }),
  name: text("name").notNull().unique(),
  color: text("color").default("#409EFF").notNull(),
  usageCount: integer("usage_count").default(0).notNull(),
  createdAt: text("created_at").notNull().default(sql`CURRENT_TIMESTAMP`),
  updatedAt: text("updated_at").notNull().default(sql`CURRENT_TIMESTAMP`)
});
var reports = sqliteTable("reports", {
  id: integer("id").primaryKey({ autoIncrement: true }),
  reporter: text("reporter").notNull(),
  deviceName: text("device_name").notNull(),
  faultTags: text("fault_tags"),
  // JSON
  durationMinutes: integer("duration_minutes"),
  faultCategory: text("fault_category").notNull(),
  description: text("description"),
  // //
  attachments: text("attachments"),
  // JSON
  isDeleted: integer("is_deleted").default(0).notNull(),
  authorId: integer("author_id").references(() => users.id),
  createdAt: text("created_at").notNull().default(sql`CURRENT_TIMESTAMP`),
  updatedAt: text("updated_at").notNull().default(sql`CURRENT_TIMESTAMP`)
});
var aiAnalyses = sqliteTable("ai_analyses", {
  id: integer("id").primaryKey({ autoIncrement: true }),
  reportId: integer("report_id").notNull().references(() => reports.id),
  analysisType: text("analysis_type", { enum: ["summary", "diagnosis", "solution"] }).notNull(),
  prompt: text("prompt").notNull(),
  response: text("response").notNull(),
  tokensUsed: integer("tokens_used"),
  createdAt: text("created_at").notNull().default(sql`CURRENT_TIMESTAMP`)
});
var aiUsageLogs = sqliteTable("ai_usage_logs", {
  id: integer("id").primaryKey({ autoIncrement: true }),
  userId: integer("user_id").notNull().references(() => users.id),
  usageDate: text("usage_date").notNull(),
  // YYYY-MM-DD
  requestCount: integer("request_count").default(0).notNull(),
  lastRequestAt: text("last_request_at")
});

// src/db/index.ts
function getDB(c) {
  if (typeof c !== "undefined" && c && c.env && c.env.DB) {
    return drizzle(c.env.DB, { schema: schema_exports });
  }
  throw new Error("\u6570\u636E\u5E93\u8FDE\u63A5\u5931\u8D25: \u672A\u627E\u5230\u6709\u6548\u7684\u6570\u636E\u5E93\u5B9E\u4F8B");
}
