ai.get("/usage", jwt({ alg: "HS256" }), zValidator("query", external_exports.object({
  startDate: external_exports.string().optional(),
  endDate: external_exports.string().optional()
})), async (c) => {
  const payload = c.get("jwtPayload");
  const { startDate, endDate } = c.req.valid("query");
  const db = getDB(c);
  try {
    const conditions = [eq(aiUsageLogs.userId, payload.userId)];
    if (startDate) {
      conditions.push(gte(aiUsageLogs.usageDate, startDate));
    }
    if (endDate) {
      conditions.push(lte(aiUsageLogs.usageDate, endDate));
    }
    const whereCondition = and2(...conditions);
    const usageResult = await db.select({
      usageDate: aiUsageLogs.usageDate,
      requestCount: aiUsageLogs.requestCount,
      lastRequestAt: aiUsageLogs.lastRequestAt
    }).from(aiUsageLogs).where(whereCondition).orderBy(desc(aiUsageLogs.usageDate));
    const totalResult = await db.select({
      totalRequests: sql`SUM(${aiUsageLogs.requestCount})`
    }).from(aiUsageLogs).where(whereCondition);
    const totalRequests = totalResult[0].totalRequests || 0;
    return c.json({
      success: true,
      message: "\u83B7\u53D6AI\u4F7F\u7528\u7EDF\u8BA1\u6210\u529F",
      data: {
        usage: usageResult,
        totalRequests
      }
    });
  } catch (error) {
    console.error("\u83B7\u53D6AI\u4F7F\u7528\u7EDF\u8BA1\u5931\u8D25:", error);
    return c.json({
      success: false,
      message: "\u83B7\u53D6AI\u4F7F\u7528\u7EDF\u8BA1\u5931\u8D25",
      data: null
    }, 500);
  }
});
ai.get("/analyses", jwt({ alg: "HS256" }), zValidator("query", external_exports.object({
  page: external_exports.coerce.number().int().min(1, "\u9875\u7801\u5FC5\u987B\u5927\u4E8E0").default(1),
  pageSize: external_exports.coerce.number().int().min(1, "\u6BCF\u9875\u6570\u91CF\u5FC5\u987B\u5927\u4E8E0").max(100, "\u6BCF\u9875\u6570\u91CF\u4E0D\u80FD\u8D85\u8FC7100").default(10),
  reportId: external_exports.number().int().positive("\u62A5\u544AID\u5FC5\u987B\u4E3A\u6B63\u6574\u6570").optional(),
  analysisType: external_exports.enum(["summary", "diagnosis", "solution"]).optional()
})), async (c) => {
  const payload = c.get("jwtPayload");
  const { page, pageSize, reportId, analysisType } = c.req.valid("query");
  const db = getDB(c);
  try {
    const conditions = [];
    if (reportId) {
      conditions.push(eq(aiAnalyses.reportId, reportId));
    }
    if (analysisType) {
      conditions.push(eq(aiAnalyses.analysisType, analysisType));
    }
    const whereCondition = conditions.length > 0 ? and2(...conditions) : void 0;
    let countQuery = db.select({ count: sql`count(*)` }).from(aiAnalyses);
    if (whereCondition) {
      countQuery = countQuery.where(whereCondition);
    }
    const countResult = await countQuery;
    const total = countResult[0].count;
    const offset = (page - 1) * pageSize;
    const analysesResult = await db.select({
      id: aiAnalyses.id,
      reportId: aiAnalyses.reportId,
      analysisType: aiAnalyses.analysisType,
      createdAt: aiAnalyses.createdAt,
      tokensUsed: aiAnalyses.tokensUsed
    }).from(aiAnalyses).where(whereCondition).orderBy(desc(aiAnalyses.createdAt)).limit(pageSize).offset(offset);
    const reportIds = analysesResult.map((a) => a.reportId);
    let reportMap = /* @__PURE__ */ new Map();
    if (reportIds.length > 0) {
      const reportsResult = await db.select({
        id: reports.id,
        deviceName: reports.deviceName,
        faultCategory: reports.faultCategory
      }).from(reports).where(
        and2(
          inArray(reports.id, reportIds),
          eq(reports.isDeleted, 0)
        )
      );
      reportsResult.forEach((report2) => {
        reportMap.set(report2.id, report2);
      });
    }
    const list = analysesResult.map((analysis) => {
      const report2 = reportMap.get(analysis.reportId);
      return {
        ...analysis,
        report: report2 ? {
          id: report2.id,
          deviceName: report2.deviceName,
          faultCategory: report2.faultCategory
        } : null
      };
    });
    return c.json({
      success: true,
      message: "\u83B7\u53D6AI\u5206\u6790\u8BB0\u5F55\u5217\u8868\u6210\u529F",
      data: {
        list,
        pagination: {
          page,
          pageSize,
          total,
          totalPages: Math.ceil(total / pageSize)
        }
      }
    });
  } catch (error) {
    console.error("\u83B7\u53D6AI\u5206\u6790\u8BB0\u5F55\u5217\u8868\u5931\u8D25:", error);
    return c.json({
      success: false,
      message: "\u83B7\u53D6AI\u5206\u6790\u8BB0\u5F55\u5217\u8868\u5931\u8D25",
      data: null
    }, 500);
  }
});
ai.get("/analyses/:id", jwt({ alg: "HS256" }), zValidator("param", external_exports.object({
  id: external_exports.coerce.number().int().positive("ID\u5FC5\u987B\u4E3A\u6B63\u6574\u6570")
})), async (c) => {
  const { id } = c.req.valid("param");
  const db = getDB(c);
  try {
    const analysisResult = await db.select().from(aiAnalyses).where(eq(aiAnalyses.id, id)).limit(1);
    if (analysisResult.length === 0) {
      return c.json({
        success: false,
        message: "\u5206\u6790\u8BB0\u5F55\u4E0D\u5B58\u5728",
        data: null
      }, 404);
    }
    const analysis = analysisResult[0];
    const reportResult = await db.select({
      id: reports.id,
      deviceName: reports.deviceName,
      faultCategory: reports.faultCategory,
      description: reports.description
    }).from(reports).where(
      and2(
        eq(reports.id, analysis.reportId),
        eq(reports.isDeleted, 0)
      )
    ).limit(1);
    const report2 = reportResult.length > 0 ? reportResult[0] : null;
    return c.json({
      success: true,
      message: "\u83B7\u53D6AI\u5206\u6790\u8BB0\u5F55\u8BE6\u60C5\u6210\u529F",
      data: {
        ...analysis,
        report: report2
      }
    });
  } catch (error) {
    console.error("\u83B7\u53D6AI\u5206\u6790\u8BB0\u5F55\u8BE6\u60C5\u5931\u8D25:", error);
    return c.json({
      success: false,
      message: "\u83B7\u53D6AI\u5206\u6790\u8BB0\u5F55\u8BE6\u60C5\u5931\u8D25",
      data: null
    }, 500);
  }
});
