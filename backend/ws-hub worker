// src/index.ts
var app = new Hono2();
app.use("*", logger());
app.use("*", prettyJSON());
app.use("*", timing());
app.use("*", secureHeaders());
app.use("*", cors({
  origin: ["http://localhost:3000", "http://localhost:5173", "https://ws-hub.pages.dev", "https://5a8a8422.ws-hub.pages.dev", "https://17f0a939.ws-hub.pages.dev", "https://c68984cd.ws-hub.pages.dev", "https://434d2f33.ws-hub.pages.dev", "https://35570b57.ws-hub.pages.dev"],
  // 
  allowMethods: ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
  allowHeaders: ["Content-Type", "Authorization"]
}));
app.route("/api/auth", auth_default);
app.route("/api/users", user_default);
app.route("/api/reports", report_default);
app.route("/api/tags", tag_default);
app.route("/api/ai", ai_default);
app.get("/health", (c) => {
  return c.json({
    success: true,
    data: {
      status: "ok",
      timestamp: (/* @__PURE__ */ new Date()).toISOString(),
      uptime: process.uptime ? process.uptime() : 0,
      environment: "undefined"
    },
    message: "Service is healthy"
  });
});
app.notFound((c) => {
  return c.json({
    success: false,
    message: "Not Found",
    error: {
      status: 404,
      text: "The requested resource was not found"
    },
    timestamp: (/* @__PURE__ */ new Date()).toISOString()
  }, 404);
});
app.onError((err, c) => {
  console.error(`[ERROR] ${err.message}`);
  return c.json({
    success: false,
    message: "Internal Server Error",
    error: {
      status: 500,
      text: err.message
    },
    timestamp: (/* @__PURE__ */ new Date()).toISOString()
  }, 500);
});
var src_default = app;
export {
  src_default as default
};
