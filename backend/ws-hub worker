      data: {
        ...analysisResult[0],
        response
      }
    });
  } catch (error) {
    console.error("AI\u5206\u6790\u5931\u8D25:", error);
    return c.json({
      success: false,
      message: "AI\u5206\u6790\u5931\u8D25",
      data: null
    }, 500);
  }
