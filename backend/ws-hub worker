  page: external_exports.coerce.number().int().min(1, "\u9875\u7801\u5FC5\u987B\u5927\u4E8E0").default(1),
  pageSize: external_exports.coerce.number().int().min(1, "\u6BCF\u9875\u6570\u91CF\u5FC5\u987B\u5927\u4E8E0").max(100, "\u6BCF\u9875\u6570\u91CF\u4E0D\u80FD\u8D85\u8FC7100").default(10),
  search: external_exports.string().optional(),
  role: external_exports.enum(["viewer", "editor", "admin"]).optional(),
  sortBy: external_exports.enum(["id", "username", "email", "role", "createdAt"]).default("createdAt"),
  sortOrder: external_exports.enum(["asc", "desc"]).default("desc")
})), async (c) => {
