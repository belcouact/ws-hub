auth.post("/login", zValidator("json", external_exports.object({
  username: external_exports.string().min(1, "\u7528\u6237\u540D\u4E0D\u80FD\u4E3A\u7A7A"),
  password: external_exports.string().min(1, "\u5BC6\u7801\u4E0D\u80FD\u4E3A\u7A7A")
})), async (c) => {
  const { username, password } = c.req.valid("json");
  const db = getDB(c);
  try {
    const result = await db.select().from(users).where(eq(users.username, username)).limit(1);
    if (result.length === 0) {
      return c.json({
        success: false,
        message: "\u7528\u6237\u540D\u6216\u5BC6\u7801\u9519\u8BEF",
        data: null
      }, 401);
    }
    const user2 = result[0];
    const isPasswordValid = await verifyPassword(password, user2.password);
    if (!isPasswordValid) {
      return c.json({
        success: false,
        message: "\u7528\u6237\u540D\u6216\u5BC6\u7801\u9519\u8BEF",
        data: null
      }, 401);
    }
    const jwtPayload = {
      sub: user2.id.toString(),
      userId: user2.id,
      username: user2.username,
      role: user2.role
    };
    const token = await sign2(jwtPayload, process.env.JWT_SECRET || "default-secret");
    return c.json({
      success: true,
      message: "\u767B\u5F55\u6210\u529F",
      data: {
        token,
        user: {
          id: user2.id,
          username: user2.username,
          email: user2.email,
          role: user2.role
        }
      }
    });
  } catch (error) {
    console.error("\u767B\u5F55\u5931\u8D25:", error);
    return c.json({
      success: false,
      message: "\u767B\u5F55\u5931\u8D25",
      data: null
    }, 500);
  }
});
