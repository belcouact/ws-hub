  id: external_exports.coerce.number().int().positive("ID\u5FC5\u987B\u4E3A\u6B63\u6574\u6570")
})), zValidator("json", external_exports.object({
  deviceName: external_exports.string().min(1, "\u8BBE\u5907\u540D\u79F0\u4E0D\u80FD\u4E3A\u7A7A"),
  faultCategory: external_exports.string().min(1, "\u6545\u969C\u7C7B\u522B\u4E0D\u80FD\u4E3A\u7A7A"),
  faultTagIds: external_exports.array(external_exports.number().int().positive("\u6807\u7B7EID\u5FC5\u987B\u4E3A\u6B63\u6574\u6570")).optional(),
  durationMinutes: external_exports.number().int().min(0, "\u7EF4\u4FEE\u65F6\u957F\u4E0D\u80FD\u4E3A\u8D1F\u6570").optional(),
  description: external_exports.string().min(1, "\u7EF4\u4FEE\u63CF\u8FF0\u4E0D\u80FD\u4E3A\u7A7A"),
  attachments: external_exports.array(external_exports.string()).optional()
})), async (c) => {
  const payload = c.get("jwtPayload");
  const { id } = c.req.valid("param");
  const { deviceName, faultCategory, faultTagIds, durationMinutes, description, attachments } = c.req.valid("json");
  const db = getDB(c);
  try {
    if (payload.role === "viewer") {
      return c.json({
        success: false,
        message: "\u6743\u9650\u4E0D\u8DB3",
        data: null
      }, 403);
    }
    const existingReport = await db.select({
      id: reports.id,
      faultTags: reports.faultTags
    }).from(reports).where(
      and2(
        eq(reports.id, id),
        eq(reports.isDeleted, 0)
      )
    ).limit(1);
    if (existingReport.length === 0) {
      return c.json({
        success: false,
        message: "\u62A5\u544A\u4E0D\u5B58\u5728",
        data: null
      }, 404);
    }
    if (faultTagIds && faultTagIds.length > 0) {
      const tagCountResult = await db.select({ count: sql`count(*)` }).from(faultTags).where(inArray(faultTags.id, faultTagIds));
      if (tagCountResult[0].count !== faultTagIds.length) {
        return c.json({
          success: false,
          message: "\u5305\u542B\u4E0D\u5B58\u5728\u7684\u6807\u7B7E",
          data: null
        }, 400);
      }
    }
    const report2 = existingReport[0];
    let oldTagIds = [];
    if (report2.faultTags) {
      try {
        oldTagIds = JSON.parse(report2.faultTags);
      } catch (e) {
        console.error("\u89E3\u6790\u539F\u6709\u6807\u7B7E\u5931\u8D25:", e);
      }
    }
    const result = await db.update(reports).set({
      deviceName,
      faultCategory,
      faultTags: faultTagIds ? JSON.stringify(faultTagIds) : null,
      durationMinutes,
      description,
      attachments: attachments ? JSON.stringify(attachments) : null
    }).where(eq(reports.id, id)).returning({
      id: reports.id,
      deviceName: reports.deviceName,
      faultCategory: reports.faultCategory,
      faultTags: reports.faultTags,
      durationMinutes: reports.durationMinutes,
      description: reports.description,
      attachments: reports.attachments,
      reporter: reports.reporter,
      createdAt: reports.createdAt
    });
    const updatedReport = result[0];
    const newTagIds = faultTagIds || [];
    const tagsToAdd = newTagIds.filter((id2) => !oldTagIds.includes(id2));
    const tagsToRemove = oldTagIds.filter((id2) => !newTagIds.includes(id2));
    if (tagsToAdd.length > 0) {
      await db.update(faultTags).set({
        usageCount: sql`${faultTags.usageCount} + 1`
      }).where(inArray(faultTags.id, tagsToAdd));
    }
    if (tagsToRemove.length > 0) {
      await db.update(faultTags).set({
        usageCount: sql`MAX(0, ${faultTags.usageCount} - 1)`
      }).where(inArray(faultTags.id, tagsToRemove));
    }
    return c.json({
      success: true,
      message: "\u66F4\u65B0\u62A5\u544A\u6210\u529F",
      data: updatedReport
    });
  } catch (error) {
    console.error("\u66F4\u65B0\u62A5\u544A\u5931\u8D25:", error);
    return c.json({
      success: false,
      message: "\u66F4\u65B0\u62A5\u544A\u5931\u8D25",
      data: null
    }, 500);
  }
});
report.delete("/:id", jwt({ alg: "HS256" }), zValidator("param", external_exports.object({
  id: external_exports.coerce.number().int().positive("ID\u5FC5\u987B\u4E3A\u6B63\u6574\u6570")
})), async (c) => {
  const payload = c.get("jwtPayload");
  const { id } = c.req.valid("param");
  const db = getDB(c);
  try {
    if (payload.role !== "admin") {
      return c.json({
        success: false,
        message: "\u6743\u9650\u4E0D\u8DB3",
        data: null
      }, 403);
    }
    const existingReport = await db.select({
      id: reports.id,
      faultTags: reports.faultTags
    }).from(reports).where(
      and2(
        eq(reports.id, id),
        eq(reports.isDeleted, 0)
      )
    ).limit(1);
    if (existingReport.length === 0) {
      return c.json({
        success: false,
        message: "\u62A5\u544A\u4E0D\u5B58\u5728",
        data: null
      }, 404);
    }
    const report2 = existingReport[0];
    let tagIds = [];
    if (report2.faultTags) {
      try {
        tagIds = JSON.parse(report2.faultTags);
      } catch (e) {
        console.error("\u89E3\u6790\u6807\u7B7E\u5931\u8D25:", e);
      }
    }
    await db.update(reports).set({
      isDeleted: 1
    }).where(eq(reports.id, id));
    if (tagIds.length > 0) {
      await db.update(faultTags).set({
        usageCount: sql`MAX(0, ${faultTags.usageCount} - 1)`
      }).where(inArray(faultTags.id, tagIds));
    }
    return c.json({
      success: true,
      message: "\u5220\u9664\u62A5\u544A\u6210\u529F",
      data: null
    });
  } catch (error) {
    console.error("\u5220\u9664\u62A5\u544A\u5931\u8D25:", error);
    return c.json({
      success: false,
      message: "\u5220\u9664\u62A5\u544A\u5931\u8D25",
      data: null
    }, 500);
  }
