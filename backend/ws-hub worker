ai.post("/diagnose", jwt({ alg: "HS256" }), zValidator("json", external_exports.object({
  reportId: external_exports.number().int().positive("\u62A5\u544AID\u5FC5\u987B\u4E3A\u6B63\u6574\u6570"),
  analysisType: external_exports.enum(["summary", "diagnosis", "solution"]).default("diagnosis"),
  customPrompt: external_exports.string().optional()
})), async (c) => {
  const payload = c.get("jwtPayload");
  const { reportId, analysisType, customPrompt } = c.req.valid("json");
  const db = getDB(c);
  try {
    const reportResult = await db.select({
      id: reports.id,
      deviceName: reports.deviceName,
      faultCategory: reports.faultCategory,
      description: reports.description,
      faultTags: reports.faultTags
    }).from(reports).where(
      and2(
        eq(reports.id, reportId),
        eq(reports.isDeleted, 0)
      )
    ).limit(1);
    if (reportResult.length === 0) {
      return c.json({
        success: false,
        message: "\u62A5\u544A\u4E0D\u5B58\u5728",
        data: null
      }, 404);
    }
    const report2 = reportResult[0];
    const today = getCurrentDate();
    const usageResult = await db.select({
      id: aiUsageLogs.id,
      requestCount: aiUsageLogs.requestCount
    }).from(aiUsageLogs).where(
      and2(
        eq(aiUsageLogs.userId, payload.userId),
        eq(aiUsageLogs.usageDate, today)
      )
    ).limit(1);
    const maxDailyRequests = 10;
    if (usageResult.length > 0) {
      const usage = usageResult[0];
      if (usage.requestCount >= maxDailyRequests) {
        return c.json({
          success: false,
          message: `\u4ECA\u65E5AI\u4F7F\u7528\u6B21\u6570\u5DF2\u8FBE\u4E0A\u9650\uFF08${maxDailyRequests}\u6B21\uFF09`,
          data: null
        }, 429);
      }
    }
    let prompt = "";
    if (customPrompt) {
      prompt = customPrompt;
    } else {
      switch (analysisType) {
        case "summary":
          prompt = `\u8BF7\u4E3A\u4EE5\u4E0B\u8BBE\u5907\u7EF4\u4FEE\u62A5\u544A\u751F\u6210\u4E00\u4E2A\u7B80\u6D01\u7684\u6458\u8981\uFF1A
